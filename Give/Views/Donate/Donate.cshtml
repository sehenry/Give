@model Give.Models.Donate

@{
    ViewBag.Title = "";
}

<div id="giverContainer" class="row col-md-12">
    <div class="rightBorder leftBorder">
        <h1>Donation Center</h1>
        <h4>Have something to donate? Want to make a monetary donation? Just post your item here, or choose Cash Donation, and
    help a family today! Cash donations will be used to purchase basic necessities for our recipients, and will be Given at our next
    event for anyone who needs it! Your donations are greatly appreciated! If you're donating an item, it will be posted to the Give Board,
    where families can search for an item they need and contact the Giver.</h4>
    </div>
</div>
    <div id="mainContainer" class="row col-md-12">
    <div class="row col-md-8 paddingForms">
        @using (Html.BeginForm("Donate", "Donate", FormMethod.Post))
            {
            <h5>Choose either Cash or Item donation</h5>
                @Html.DropDownListFor(model => model.DonationType.Keys, new SelectList(Model.DonationType, "Key", "Value"), new { @class = "form-control" })
                <h5>Enter the amount you'd like to donate</h5>
                @Html.TextBoxFor(model => model.CashDonation, new { @class = "form-control", @placeholder = "Cash Donation Amount" })
                <h5>Please enter the item you're donating</h5>
                @Html.TextBoxFor(model => model.ItemDonation, new { @class = "form-control", @placeholder = "Item Name and Description" })

        }
    </div>
    <div class="col-md-4">
        <img src="data:image/png;base64," width="300" height="300" alt="Donate" />
    </div>

    <div class="form-group">
        <div class="">
            <input type="submit" class="btn btn-secondary btn-lg" value="Submit Donation" />
        </div>
    </div>
</div>

